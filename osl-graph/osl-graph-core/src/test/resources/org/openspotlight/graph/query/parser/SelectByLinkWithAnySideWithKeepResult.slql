select org.openspotlight.graph.test.domain.JavaInterface, org.openspotlight.graph.test.domain.JavaTypeMethod
	where
		org.openspotlight.graph.test.domain.JavaTypeMethod
			property caption *... "get"

select org.openspotlight.graph.test.domain.JavaInterface, org.openspotlight.graph.test.domain.JavaTypeMethod.*
	by link
		org.openspotlight.graph.test.domain.TypeContainsMethod (a,b,both);