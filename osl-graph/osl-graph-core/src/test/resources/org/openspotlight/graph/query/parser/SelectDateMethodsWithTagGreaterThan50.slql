select org.openspotlight.graph.test.domain.node.JavaType.*
	where
		org.openspotlight.graph.test.domain.node.JavaType.*
			property caption == "java.util.Date"

select org.openspotlight.graph.test.domain.node.JavaTypeMethod
	by link org.openspotlight.graph.test.domain.link.TypeContainsMethod (b)
	where
		linkType org.openspotlight.graph.test.domain.link.TypeContainsMethod
			property tag > 50;