select org.openspotlight.graph.test.domain.JavaPackage, org.openspotlight.graph.test.domain.JavaType.*
	where
		org.openspotlight.graph.test.domain.JavaType
			property caption == "java.util"
		org.openspotlight.graph.test.domain.JavaType.*
			property caption == "java.util.Collection"


select org.openspotlight.graph.test.domain.JavaType.*, org.openspotlight.graph.test.domain.JavaTypeMethod
	by link
		org.openspotlight.graph.test.domain.TypeContainsMethod (b),
		org.openspotlight.graph.test.domain.PackageContainsType (b);
