select org.openspotlight.graph.test.domain.node.JavaPackage, org.openspotlight.graph.test.domain.node.JavaType.*
	where
		org.openspotlight.graph.test.domain.node.JavaType
			property caption == "java.util"
		org.openspotlight.graph.test.domain.node.JavaType.*
			property caption == "java.util.Collection"


select org.openspotlight.graph.test.domain.node.JavaType.*, org.openspotlight.graph.test.domain.node.JavaTypeMethod
	by link
		org.openspotlight.graph.test.domain.link.TypeContainsMethod (b),
		org.openspotlight.graph.test.domain.link.PackageContainsType (b);
