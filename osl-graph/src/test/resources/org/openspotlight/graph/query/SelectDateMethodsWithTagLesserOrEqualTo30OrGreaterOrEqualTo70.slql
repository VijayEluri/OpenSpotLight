select org.openspotlight.graph.query.JavaType.*
	where
		org.openspotlight.graph.query.JavaType.*
			property caption == "java.util.Date";

select org.openspotlight.graph.query.JavaTypeMethod
	by link org.openspotlight.graph.query.TypeContainsMethod (b);
	where
		linkType org.openspotlight.graph.query.TypeContainsMethod
			property tag <= 30 ||
			property tag >= 70;