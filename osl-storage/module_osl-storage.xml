<?xml version="1.0" encoding="UTF-8"?>
<project name="module_osl-storage" default="compile.module.osl-storage">
  <dirname property="module.osl-storage.basedir" file="${ant.file.module_osl-storage}"/>
  
  <property name="module.jdk.home.osl-storage" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.osl-storage" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.osl-storage" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.osl-storage" value="${compiler.args}"/>
  
  <property name="osl-storage.output.dir" value="${module.osl-storage.basedir}/out/production/osl-storage"/>
  <property name="osl-storage.testoutput.dir" value="${module.osl-storage.basedir}/out/test/osl-storage"/>
  
  <path id="osl-storage.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="osl-storage.module.classpath">
    <path refid="${module.jdk.classpath.osl-storage}"/>
  </path>
  
  <path id="osl-storage.runtime.module.classpath">
    <pathelement location="${osl-storage.output.dir}"/>
    <pathelement location="${osl-storage.testoutput.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.osl-storage">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.osl-storage">
    <patternset refid="excluded.from.module.osl-storage"/>
  </patternset>
  
  
  <target name="compile.module.osl-storage" depends="compile.module.osl-storage.production,compile.module.osl-storage.tests" description="Compile module osl-storage"/>
  
  <target name="compile.module.osl-storage.production" description="Compile module osl-storage; production classes"/>
  
  <target name="compile.module.osl-storage.tests" depends="compile.module.osl-storage.production" description="compile module osl-storage; test classes" unless="skip.tests"/>
  
  <target name="clean.module.osl-storage" description="cleanup module">
    <delete dir="${osl-storage.output.dir}"/>
    <delete dir="${osl-storage.testoutput.dir}"/>
  </target>
</project>