select org.openspotlight.graph.query.JavaInterface
	where
		org.openspotlight.graph.query.JavaInterface
			property caption == "java.util.SortedSet"
	keep result;

select org.openspotlight.graph.query.JavaInterface
	by link org.openspotlight.graph.query.JavaInterfaceHierarchy (b)
	executing n times
	keep result;