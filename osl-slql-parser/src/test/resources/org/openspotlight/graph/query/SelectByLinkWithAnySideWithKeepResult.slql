select org.openspotlight.graph.query.JavaInterface, org.openspotlight.graph.query.JavaTypeMethod
	where
		org.openspotlight.graph.query.JavaTypeMethod
			property caption *... "get";

select org.openspotlight.graph.query.JavaInterface, org.openspotlight.graph.query.JavaTypeMethod.*
	by link
		org.openspotlight.graph.query.TypeContainsMethod (a,b,both);